[
  {
    "name": "filesystem",
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp/agentos-sandbox"],
    "enabled": true
  },
  {
    "name": "fetch",
    "command": "uvx",
    "args": ["mcp-server-fetch"],
    "enabled": true
  },
  {
    "name": "memory",
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-memory"],
    "enabled": true
  },
  {
    "name": "sqlite",
    "command": "npx",
